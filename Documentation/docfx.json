{
    "metadata": [
        {
            "src": [
                {
                    "src": "../src",
                    "files": [
                        "**/*.csproj"
                    ]
                }
            ],
            "dest": "api"
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "**/*.{md,yml}"
                ],
                "exclude": [
                    "_site/**"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**"
                ]
            }
        ],
        "output": "_site",
        "template": [
            "default",
            "unity"
        ],
        "globalMetadata": {
            "_appName": "Gaze Interaction System",
            "_appTitle": "Gaze Interaction System",
            "_enableSearch": true,
            "pdf": true
        },
        "sitemap": {
            "baseUrl": "https://tomazsaraiva.github.io/unity-gaze-interaction"
        }
    }
}